<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="347dp"
        android:layout_height="40dp"
        android:layout_marginTop="176dp"
        android:text="Welcome to your Kitchen Kompanion"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="41dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="101dp"
        android:layout_height="91dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="436dp"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        android:onClick="openmain2"
        android:scaleType="fitStart"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/grocery_list_icon_25" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="96dp"
        android:layout_height="95dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="436dp"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        android:onClick="openmain2"
        android:scaleType="fitStart"
        app:layout_constraintStart_toEndOf="@+id/imageButton4"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fridge_freezer" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="87dp"
        android:layout_height="87dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="260dp"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        android:onClick="openmain2"
        android:scaleType="fitStart"
        app:layout_constraintStart_toEndOf="@+id/imageButton5"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kitchen_cabinet" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="102dp"
        android:layout_height="96dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="256dp"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:onClick="openmain3"
        android:scaleType="fitStart"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/chef_icon_png_13" />

</androidx.constraintlayout.widget.ConstraintLayout>